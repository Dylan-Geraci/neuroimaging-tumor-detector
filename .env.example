# Application
APP_ENVIRONMENT=development
APP_SECRET_KEY=generate-with-python-secrets-token-urlsafe-32
APP_API_KEYS=key1,key2,key3

# CORS
APP_CORS_ORIGINS=http://localhost:5173,http://localhost:8000

# Database
APP_DATABASE_URL=sqlite:///./predictions.db

# Security
APP_RATE_LIMIT_ENABLED=false
APP_RATE_LIMIT_PER_MINUTE=10
APP_MAX_FILE_SIZE_MB=50
APP_MAX_BATCH_SIZE=20

# Model
APP_MODEL_PATH=models/best_model.pth
APP_MODEL_SOURCE=local

# Server
APP_HOST=0.0.0.0
APP_PORT=8000
APP_LOG_LEVEL=INFO

# PostgreSQL (only needed for production/docker-compose)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=change-me-in-production
POSTGRES_DB=predictions
